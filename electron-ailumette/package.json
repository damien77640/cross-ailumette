{
  "name": "electron-ailumette",
  "version": "0.1.0",
  "private": true,
  "main": "src/start.js",
  "homepage": "./",
  "build": {
    "appId": "app-test",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "app-win",
      "iconUrl": "path-to-icon.png"
    },
    "mac": {
      "category": "app-mac",
      "iconUrl": "path-to-icon.png"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron": "^11.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
